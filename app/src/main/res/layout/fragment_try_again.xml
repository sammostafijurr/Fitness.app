<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TryAgainFragment" >

    <ImageView
        android:id="@+id/tryAgainView"
        android:layout_width="260dp"
        android:layout_height="312dp"
        android:layout_marginTop="48dp"
        android:contentDescription="@string/content_dis"
        android:src="@drawable/try_againg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tryagainButton"
        android:layout_width="135dp"
        android:layout_height="56dp"
        android:backgroundTint="#DA0F0F"
        android:text="@string/try_again"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tryAgainView" />
</androidx.constraintlayout.widget.ConstraintLayout>