<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WellDoneFragment" >

    <ImageView
        android:id="@+id/welldone"
        android:layout_width="416dp"
        android:layout_height="576dp"
        android:contentDescription="@string/app_name"
        android:src="@drawable/welldone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/nextworkoutButton"
        android:layout_width="175dp"
        android:layout_height="49dp"
        android:backgroundTint="#201D1D"
        android:text="@string/next_work"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welldone" />
</androidx.constraintlayout.widget.ConstraintLayout>